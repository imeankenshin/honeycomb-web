{
  "name": "web",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "solid-start dev",
    "build": "solid-start build",
    "start": "solid-start start",
    "lint": "eslint --fix .",
    "format": "prettier --write ."
  },
  "type": "module",
  "devDependencies": {
    "@pandacss/dev": "^0.20.1",
    "@types/node": "^18.17.5",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.14.54",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.28",
    "prettier": "^3.1.0",
    "solid-start-node": "^0.3.10",
    "typescript": "^4.9.5",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@fontsource/ibm-plex-mono": "^5.0.8",
    "@fontsource/ibm-plex-sans": "^5.0.17",
    "@solidjs/meta": "^0.29.1",
    "@solidjs/router": "^0.8.3",
    "clsx": "^2.0.0",
    "solid-js": "^1.8.5",
    "solid-start": "^0.3.10"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "solid",
      "prettier",
      "jsx-a11y",
      "unicorn"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:solid/recommended"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "kebabCase"
        }
      ],
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
